<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
                      http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
  version="2.0">
  <persistence-unit name="LaboratorioPU" transaction-type="RESOURCE_LOCAL">
     <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
     <class>udu.usa.laboratorio.modelo.Categoria</class>
     <class>udu.usa.laboratorio.modelo.Dependencia</class>
     <class>udu.usa.laboratorio.modelo.Programa</class>
     <class>udu.usa.laboratorio.modelo.Auxiliar</class>
     <class>udu.usa.laboratorio.modelo.Docente</class>
     <class>udu.usa.laboratorio.modelo.Usuario</class>
     <class>udu.usa.laboratorio.modelo.Activo</class>
     <class>udu.usa.laboratorio.modelo.MantenimientoPreventivo</class>
     <class>udu.usa.laboratorio.modelo.Mantenimiento</class>
     <class>udu.usa.laboratorio.modelo.DetallePrestamo</class>
     <class>udu.usa.laboratorio.modelo.Prestamo</class>
    
